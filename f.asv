function z = f(x,y,t)
% Forcing term for test function

EXPONENT_1 = 1;
EXPONENT_2 = 2;
TRIG = 3;

global solution

if solution == TRIG
    z = (1024 * x^2 + 64 * y^2) * cos(16*x^2 + 4y^2 + t) + (4*x^2 + 4*y^2) * sin(x^2 + y^2 + t);
elseif solution == EXPONENT_1
    z = - exp(x+y+t);
elseif solution == EXPONENT_2
    z = - 12 * exp(3*x + 2*y + t);
end

end

