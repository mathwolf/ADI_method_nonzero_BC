function x = psi1(y, domain)

CIRCLE = 1;
ELLIPSE = 2;
DIAMOND = 3;
ELL = 4;

% Lower boundary of domain wrt y
if domain == CIRCLE
    x = - sqrt(1. - y^2);
elseif domain == ELLIPSE
    x = - sqrt(1. - (2.*y)^2);
elseif domain == DIAMOND
    if (-1 <= ) && (x <= 0)
        y = -x - 1;
    elseif (0 <= x) && (x <= 1)
        y = x - 1;
    else
        y = 0;
    end

end
end

