function x = eta1(y,z)

% Constants used to switch between different domains for testing.
SPHERE = 1;
ELLIPSOID = 2;
DIAMOND = 3;
ELL = 4;
RECTANGLE = 5;
DIAMOND_2 = 6;
CUBE = 7;
global domain

if domain == SPHERE
    x = - sqrt(1. - y^2 - z^2);
elseif domain == ELLIPSOID
    x = - sqrt(1. - 4.*y^2 - 16.*z^2);
elseif domain == CUBE
    x = 0;
elseif domain == RECTANGLE
    z = -0.25;
end

end